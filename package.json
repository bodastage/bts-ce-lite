{
  "name": "BTS-CE-Lite",
  "version": "0.1.2",
  "description": "BTS-CE-Lite is a telecommunication network management application",
  "private": true,
  "homepage": "./",
  "scripts": {
    "electron": "electron .",
    "electron-dev-unix": "ELECTRON_START_URL=http://localhost:3000 electron .",
    "electron-dev": "set ELECTRON_START_URL=http://localhost:3000 && electron .",
    "start": "set PORT=3000 && react-scripts start",
    "react:build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron:dev": "nf start",
    "electron-rebuild": "electron-rebuild -f -w sqlite3",
    "pack": "npx electron-builder build --dir -c.extraMetadata.main=src/electron-start.js",
    "dist-all": "npx electron-builder build --x64 --macos --win --linux -c.extraMetadata.main=src/electron-start.js",
    "dist": "npx electron-builder",
    "postinstall": "npx electron-builder install-app-deps",
    "electron:local": "npm run react:build && electron .",
    "electron:linux": "npm run react:build && npx electron-builder build --linux",
    "electron:linux:arm": "npm run react:build && npx electron-builder build --linux --armv7l",
    "electron:win": "npm run react:build && npx electron-builder build --windows",
    "electron:mac": "npm run react:build && npx electron-builder build --mac",
    "release": "npm run react:build && npx electron-builder build",
    "release:all": "npm run react:build && npx electron-builder build -mwl -p always",
    "release:mac": "npm run react:build && npx electron-builder build --mac -p always",
    "release:linux": "npm run react:build && npx electron-builder build --linux -p always",
    "release:linux:arm": "npm run react:build && npx electron-builder build --linux --armv7l -p always"
  },
  "main": "src/electron-start.js",
  "repository": "https://github.com/bodastage/bts-ce-lite",
  "keywords": [
    "Boda Telecom Suite",
    "telecommunication",
    "bodastage-solutions",
    "bts-ce-lite",
    "boda-ce-lite"
  ],
  "eslintConfig": {
    "extends": "react-app"
