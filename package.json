  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "com.bodastage.btscelite",
    "productName": "BTS-CE-Lite",
    "copyright": "Copyright Â© 2019 $npm_package_author_name",
    "files": [
      "./build/**/*",
      "./src/electron-start.js",
      "./background/parse_cm_dumps.html",
      "package.json"
    ],
    "extraResources": [
      "libraries"
    ],
    "extraMetadata": {
      "main": "./src/electron-start.js"
    },
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "NSIS",
      "icon": "build/icon.ico"
    },
    "directories": {
      "buildResources": "assets"
    },
    "publish": {
      "provider": "github",
      "owner": "bodastage",
      "repo": "bts-ce-lite",
      "private": false
    }
