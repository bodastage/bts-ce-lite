{
  "name": "Boda-Lite",
  "version": "1.0.0-beta",
  "description": "Boda-Lite is a telecommunication network management application",
  "private": true,
  "homepage": "./",
  "scripts": {
    "migrate": "npx sequelize-cli db:migrate",
    "electron": "electron .",
    "electron-dev-unix": "ELECTRON_ENV=dev ELECTRON_START_URL=http://localhost:3000 electron .",
    "electron-dev": "set ELECTRON_START_URL=http://localhost:3000 && electron .",
    "start": "set PORT=3000 && react-scripts --max_old_space_size=4096 start",
    "start-rewired": "set PORT=3000 && react-app-rewired --max_old_space_size=4096 start",
    "react:build": "react-scripts --max_old_space_size=4096 build",
    "test": "react-scripts --max_old_space_size=4096 test",
    "eject": "react-scripts eject",
    "electron:dev": "nf start",
    "electron-rebuild": "electron-rebuild -f -w sqlite3",
    "electron:pack": "build --dir",
    "pack": "npx electron-builder build --dir -c.extraMetadata.main=main.js",
    "dist-all": "npx electron-builder build --x64 --macos --win --linux -c.extraMetadata.main=main.js",
    "electron:dist": "npm run react:build && npx electron-builder",
    "postinstall": "npx electron-builder install-app-deps",
    "electron:linux-snap": "npm run react:build && build --linux snap",
    "electron:local": "npm run react:build && electron .",
    "electron:linux": "npm run react:build && npx electron-builder build --linux",
    "electron:linux:arm": "npm run react:build && npx electron-builder build --linux --armv7l",
    "electron:win": "npm run react:build && npx electron-builder build --windows",
    "electron:mac": "npm run react:build && npx electron-builder build --mac",
    "release": "npm run react:build && npx electron-builder build",
    "release:all": "npm run react:build && npx electron-builder build -mwl -p always",
    "release:mac": "npm run build && npm run react:build && npx electron-builder build --mac -p always -c.extraMetadata.main=main.js",
    "release:linux": "npm run build && npm run react:build && npx electron-builder build --linux -p always",
    "release:linux:arm": "npm run react:build && npx electron-builder build --linux --armv7l -p always",
    "compile": "electron-webpack --config webpack.main.config.js",
    "compile-main": "webpack --config webpack.main.config.js",
    "build:main": "webpack --config webpack.main.config.js",
    "build:preload": "webpack --config webpack.preload.config.js",
    "build": "npm run build:main && npm run build:preload"
  },
  "main": "main.js",
  "repository": "https://github.com/bodastage/bts-ce-lite",
  "keywords": [
    "Boda Telecom Suite",
    "telecommunication",
    "bodastage-solutions",
    "boda-ce-lite",
    "boda-lite",
    "bts-ce-lite"
  ],
