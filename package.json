  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "extends": null,
    "appId": "com.bodastage.bodalite",
    "productName": "Boda-Lite",
    "copyright": "Copyright Â© 2023 $npm_package_author_name",
    "files": [
      "./build/**/*",
      "./main.js",
      "./preload.js",
      "./backend/**/*",
      "./node_modules",
      "./package.json",
      "./db/**/*",
      "./boda-lite.sqlite"
    ],
    "extraResources": [
      "./assets/**/*"
    ],
    "extraMetadata": {
      "main": "./main.js"
    },
    "dmg": {
      "icon": "build/icon.icns",
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "category": "Science",
      "packageCategory": "Science",
      "target": [
        "AppImage",
        "deb",
        "zip",
        "snap",
        "rpm"
      ],
      "icon": "./build/icon.icns",
      "synopsis": "Boda-Lite Desktop App",
      "description": "Boda-Lite is a telecommunication network management desktop client."
    },
    "deb": {
      "synopsis": "Boda-Lite Desktop App",
      "afterInstall": "build/debian-after-install.sh"
    },
    "win": {
      "target": [
        "nsis",
        "portable",
        "zip"
      ],
      "icon": "build/icon.ico"
    },
    "directories": {
      "buildResources": "assets"
    },
    "publish": {
      "provider": "github",
      "owner": "bodastage",
      "repo": "bts-ce-lite",
      "private": false
    }
  },
  "contributors": [
    {
      "name": "Emmanuel Robert Ssebaggala",
      "email": "emmanuel.ssebaggala@bodastage.com",
      "url": "https://github.com/erssebagala"
    }
  ],
  "browser": {
    "http": false,
    "https": false,
    "net": false,
    "stream": false,
    "tls": false
  },
  "extraResources": [
    {
      "from": "./node_modules/umzug/**/*",
      "to": "./node_modules/umzug/**/*",
      "filter": [
        "**/*"
      ]
    }
  ]
